<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="pe.edu.pucp.proyecto1_appocalipsis">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".usuario.SolicitudesDePrestamo"
            android:parentActivityName=".usuario.MenuPrincipalUsuario"></activity>
        <activity android:name=".usuario.HistorialDePrestamo"
            android:parentActivityName=".usuario.MenuPrincipalUsuario"/>
        <activity android:name=".usuario.MenuPrincipalUsuario" />
        <activity android:name=".usuario.ReservaDispositivos"
            android:parentActivityName=".usuario.MasDetalles"/>
        <activity android:name=".usuario.MasDetalles"
            android:parentActivityName=".usuario.ListarDispositivos"/>
        <activity android:name=".usuario.ListarDispositivos"
            android:parentActivityName=".usuario.MenuPrincipalUsuario"/>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>