<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".admin.MenuPrincipalAdmin">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:gravity="center"
        android:orientation="vertical">

        <TextView
            android:id="@+id/textView5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="32dp"
            android:text="Bienvenido al menu principal ADMIN"
            app:layout_constraintBottom_toTopOf="@+id/reserva"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" />

        <Button
            android:id="@+id/gestionar"
            android:layout_width="322dp"
            android:layout_height="83dp"
            android:layout_marginTop="32dp"
            android:text="Gestionar Dispositivos"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" />

        <Button
            android:id="@+id/reserva"
            android:layout_width="363dp"
            android:layout_height="67dp"
            android:layout_marginTop="32dp"
            android:text="Pedidos de Reserva"
            app:layout_constraintBottom_toTopOf="@+id/gestionar"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.497"
            app:layout_constraintStart_toStartOf="parent" />

        <Button
            android:id="@+id/cerrarSessionAdmin"
            android:layout_width="362dp"
            android:layout_height="67dp"
            android:layout_marginTop="32dp"
            android:layout_marginBottom="12dp"
            android:text="Cerrar Sesion"
            app:layout_constraintBottom_toTopOf="@+id/gestionar"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.498"
            app:layout_constraintStart_toStartOf="parent" />
    </LinearLayout>


</androidx.constraintlayout.widget.ConstraintLayout>